    "test:e2e:headed": "playwright test --headed",
    "test:e2e:install": "playwright install"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1", 
    "react-router-dom": "^6.28.0",
    "axios": "^1.7.9",
    "lucide-react": "^0.468.0",
